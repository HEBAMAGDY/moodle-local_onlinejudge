<?xml version="1.0" encoding="UTF-8" ?>
<XMLDB PATH="local/onlinejudge2/db" VERSION="20110622" COMMENT="XMLDB file for Moodle local/onlinejudge2"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="../../../lib/xmldb/xmldb.xsd"
>
  <TABLES>
    <TABLE NAME="onlinejudge2_tasks" COMMENT="Details of tasks">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="20" NOTNULL="true" UNSIGNED="true" SEQUENCE="true" NEXT="coursemodule"/>
        <FIELD NAME="coursemodule" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" SEQUENCE="false" COMMENT="Source coursemodule ID" PREVIOUS="id" NEXT="userid"/>
        <FIELD NAME="userid" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" SEQUENCE="false" COMMENT="Related user" PREVIOUS="coursemodule" NEXT="language"/>
        <FIELD NAME="language" TYPE="char" LENGTH="255" NOTNULL="true" SEQUENCE="false" COMMENT="Programming language" PREVIOUS="userid" NEXT="memlimit"/>
        <FIELD NAME="memlimit" TYPE="int" LENGTH="20" NOTNULL="true" UNSIGNED="true" DEFAULT="1048576" SEQUENCE="false" COMMENT="max memory" PREVIOUS="language" NEXT="cpulimit"/>
        <FIELD NAME="cpulimit" TYPE="int" LENGTH="20" NOTNULL="true" UNSIGNED="true" DEFAULT="1" SEQUENCE="false" COMMENT="max cpu" PREVIOUS="memlimit" NEXT="input"/>
        <FIELD NAME="input" TYPE="text" LENGTH="big" NOTNULL="false" SEQUENCE="false" COMMENT="input data" PREVIOUS="cpulimit" NEXT="output"/>
        <FIELD NAME="output" TYPE="text" LENGTH="big" NOTNULL="false" SEQUENCE="false" COMMENT="output data" PREVIOUS="input" NEXT="answer"/>
        <FIELD NAME="answer" TYPE="text" LENGTH="big" NOTNULL="false" SEQUENCE="false" COMMENT="Output of user's program" PREVIOUS="output" NEXT="compileonly"/>
        <FIELD NAME="compileonly" TYPE="int" LENGTH="4" NOTNULL="true" UNSIGNED="true" DEFAULT="0" SEQUENCE="false" PREVIOUS="answer" NEXT="status"/>
        <FIELD NAME="status" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" DEFAULT="0" SEQUENCE="false" PREVIOUS="compileonly" NEXT="info_teacher"/>
        <FIELD NAME="info_teacher" TYPE="text" LENGTH="small" NOTNULL="false" SEQUENCE="false" COMMENT="Program runtime/compiler error, message seen by teacher" PREVIOUS="status" NEXT="info_student"/>
        <FIELD NAME="info_student" TYPE="text" LENGTH="small" NOTNULL="false" SEQUENCE="false" COMMENT="Program runtime/compiler error, the message seen by student." PREVIOUS="info_teacher" NEXT="cpuusage"/>
        <FIELD NAME="cpuusage" TYPE="int" LENGTH="10" NOTNULL="false" UNSIGNED="true" SEQUENCE="false" PREVIOUS="info_student" NEXT="memusage"/>
        <FIELD NAME="memusage" TYPE="int" LENGTH="10" NOTNULL="false" UNSIGNED="true" SEQUENCE="false" PREVIOUS="cpuusage" NEXT="submittime"/>
        <FIELD NAME="submittime" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" DEFAULT="0" SEQUENCE="false" PREVIOUS="memusage" NEXT="judgetime"/>
        <FIELD NAME="judgetime" TYPE="int" LENGTH="10" NOTNULL="false" UNSIGNED="true" SEQUENCE="false" PREVIOUS="submittime" NEXT="var1"/>
        <FIELD NAME="var1" TYPE="char" LENGTH="255" NOTNULL="false" SEQUENCE="false" PREVIOUS="judgetime" NEXT="var2"/>
        <FIELD NAME="var2" TYPE="char" LENGTH="255" NOTNULL="false" SEQUENCE="false" PREVIOUS="var1" NEXT="var3"/>
        <FIELD NAME="var3" TYPE="char" LENGTH="255" NOTNULL="false" SEQUENCE="false" PREVIOUS="var2" NEXT="var4"/>
        <FIELD NAME="var4" TYPE="char" LENGTH="255" NOTNULL="false" SEQUENCE="false" PREVIOUS="var3" NEXT="deleted"/>
        <FIELD NAME="deleted" TYPE="int" LENGTH="4" NOTNULL="true" UNSIGNED="true" DEFAULT="0" SEQUENCE="false" PREVIOUS="var4"/>
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id" NEXT="coursemodule"/>
        <KEY NAME="coursemodule" TYPE="foreign" FIELDS="coursemodule" REFTABLE="course_modules" REFFIELDS="id" PREVIOUS="primary" NEXT="userid"/>
        <KEY NAME="userid" TYPE="foreign" FIELDS="userid" REFTABLE="user" REFFIELDS="id" PREVIOUS="coursemodule"/>
      </KEYS>
      <INDEXES>
        <INDEX NAME="status" UNIQUE="false" FIELDS="status"/>
      </INDEXES>
    </TABLE>
  </TABLES>
</XMLDB>