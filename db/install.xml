<?xml version="1.0" encoding="UTF-8" ?>
<XMLDB PATH="local/onlinejudge2/db" VERSION="20110329" COMMENT="XMLDB file for Moodle local/onlinejudge2"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="../../../lib/xmldb/xmldb.xsd"
>
  <TABLES>
    <TABLE NAME="onlinejudge_task" COMMENT="put the information into task table" NEXT="onlinejudge_result">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" SEQUENCE="true" NEXT="taskname"/>
        <FIELD NAME="taskname" TYPE="char" LENGTH="100" NOTNULL="false" SEQUENCE="false" COMMENT="task name" PREVIOUS="id" NEXT="judgename"/>
        <FIELD NAME="judgename" TYPE="int" LENGTH="20" NOTNULL="true" UNSIGNED="true" SEQUENCE="false" COMMENT="the compiler name and language." PREVIOUS="taskname" NEXT="memlimit"/>
        <FIELD NAME="memlimit" TYPE="int" LENGTH="20" NOTNULL="true" UNSIGNED="true" DEFAULT="1048576" SEQUENCE="false" COMMENT="max memory" PREVIOUS="judgename" NEXT="cpulimit"/>
        <FIELD NAME="cpulimit" TYPE="int" LENGTH="20" NOTNULL="true" UNSIGNED="true" DEFAULT="1" SEQUENCE="false" COMMENT="max cpu" PREVIOUS="memlimit" NEXT="input"/>
        <FIELD NAME="input" TYPE="text" LENGTH="small" NOTNULL="false" SEQUENCE="false" COMMENT="input" PREVIOUS="cpulimit" NEXT="output"/>
        <FIELD NAME="output" TYPE="text" LENGTH="small" NOTNULL="false" SEQUENCE="false" COMMENT="output" PREVIOUS="input" NEXT="usefile"/>
        <FIELD NAME="usefile" TYPE="int" LENGTH="20" NOTNULL="false" UNSIGNED="true" SEQUENCE="false" COMMENT="usefile?" PREVIOUS="output" NEXT="inputfile"/>
        <FIELD NAME="inputfile" TYPE="text" LENGTH="small" NOTNULL="false" SEQUENCE="false" COMMENT="inputfile" PREVIOUS="usefile" NEXT="outputfile"/>
        <FIELD NAME="outputfile" TYPE="text" LENGTH="small" NOTNULL="false" SEQUENCE="false" PREVIOUS="inputfile"/>
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
      </KEYS>
    </TABLE>
    <TABLE NAME="onlinejudge_result" COMMENT="the result task" PREVIOUS="onlinejudge_task">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" SEQUENCE="true" NEXT="taskid"/>
        <FIELD NAME="taskid" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" SEQUENCE="false" COMMENT="foreign key reference to task.id" PREVIOUS="id" NEXT="taskname"/>
        <FIELD NAME="taskname" TYPE="char" LENGTH="100" NOTNULL="false" SEQUENCE="false" PREVIOUS="taskid" NEXT="judgename"/>
        <FIELD NAME="judgename" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" SEQUENCE="false" PREVIOUS="taskname" NEXT="memlimit"/>
        <FIELD NAME="memlimit" TYPE="int" LENGTH="20" NOTNULL="true" UNSIGNED="true" DEFAULT="1048576" SEQUENCE="false" COMMENT="max memory" PREVIOUS="judgename" NEXT="cpulimit"/>
        <FIELD NAME="cpulimit" TYPE="int" LENGTH="20" NOTNULL="true" UNSIGNED="true" DEFAULT="1" SEQUENCE="false" COMMENT="max cpu." PREVIOUS="memlimit" NEXT="input"/>
        <FIELD NAME="input" TYPE="text" LENGTH="small" NOTNULL="false" SEQUENCE="false" PREVIOUS="cpulimit" NEXT="output"/>
        <FIELD NAME="output" TYPE="text" LENGTH="small" NOTNULL="false" SEQUENCE="false" PREVIOUS="input" NEXT="usefile"/>
        <FIELD NAME="usefile" TYPE="int" LENGTH="20" NOTNULL="false" UNSIGNED="true" SEQUENCE="false" PREVIOUS="output" NEXT="inputfile"/>
        <FIELD NAME="inputfile" TYPE="text" LENGTH="small" NOTNULL="false" SEQUENCE="false" PREVIOUS="usefile" NEXT="outputfile"/>
        <FIELD NAME="outputfile" TYPE="text" LENGTH="small" NOTNULL="false" SEQUENCE="false" PREVIOUS="inputfile" NEXT="judged"/>
        <FIELD NAME="judged" TYPE="int" LENGTH="20" NOTNULL="true" UNSIGNED="true" DEFAULT="0" SEQUENCE="false" COMMENT="judged or not" PREVIOUS="outputfile" NEXT="status"/>
        <FIELD NAME="status" TYPE="int" LENGTH="20" NOTNULL="false" UNSIGNED="true" SEQUENCE="false" COMMENT="success or fail or exception" PREVIOUS="judged" NEXT="info"/>
        <FIELD NAME="info" TYPE="text" LENGTH="small" NOTNULL="false" SEQUENCE="false" COMMENT="extra information to describe the status." PREVIOUS="status" NEXT="starttime"/>
        <FIELD NAME="starttime" TYPE="text" LENGTH="small" NOTNULL="false" SEQUENCE="false" PREVIOUS="info" NEXT="endtime"/>
        <FIELD NAME="endtime" TYPE="text" LENGTH="small" NOTNULL="false" SEQUENCE="false" PREVIOUS="starttime"/>
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
      </KEYS>
    </TABLE>
  </TABLES>
</XMLDB>